              <xAxis>
                  <NumberAxis label="Size" prefHeight="40.0" prefWidth="360.0" side="BOTTOM" />
              </xAxis>
              <yAxis>
                  <NumberAxis label="Elapsed nanos" prefHeight="329.0" prefWidth="110.0" side="LEFT" />
              </yAxis>
          </LineChart>
         <columnConstraints>
            <ColumnConstraints />
            <ColumnConstraints />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints />
            <RowConstraints />
         </rowConstraints>
      </GridPane>
   </center>

    <right>
        <Pane prefHeight="400" prefWidth="15" BorderPane.alignment="CENTER" />
