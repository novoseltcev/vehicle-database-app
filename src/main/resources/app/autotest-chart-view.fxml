          </LineChart>
          <LineChart fx:id="chartMeanRm" legendSide="RIGHT" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400" prefWidth="500" title="Remove: mean time" BorderPane.alignment="CENTER" GridPane.columnIndex="1">
              <xAxis>
                  <NumberAxis label="Size" prefHeight="40.0" prefWidth="360.0" side="BOTTOM" />
              </xAxis>
              <yAxis>
                  <NumberAxis label="Elapsed nanos" prefHeight="329.0" prefWidth="110.0" side="LEFT" />
